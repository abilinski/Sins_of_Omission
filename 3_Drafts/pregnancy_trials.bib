	journal = {Open Forum Infectious Diseases},
	author = {Short, William R and Miller, Emily S and Simone, Joanne and Statton, Anne and Finocchario-Kessler, Sarah and Lampe, Margaret},
	month = jul,
	year = {2024},
	pmid = {39130080},
	pmcid = {PMC11310583},
	pages = {ofae423},
