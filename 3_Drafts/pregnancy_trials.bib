	pmcid = {PMC8986277},
	pages = {188--195},
	file = {PubMed Central Full Text PDF:/Users/alyssabilinsi/Zotero/storage/8FUCSS4P/Wang et al. - 2022 - The association between pregnancy and COVID-19 A .pdf:application/pdf},
}


